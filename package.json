{
  "name": "classifyed",
  "version": "1.3.1",
  "description": "A tiny yet powerful lib to create extensible JS Classes, with an elegant way of calling parent methods.",
  "main": "classifyed.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/duzun/classifyed.js.git"
  },
  "reveal": true,
  "keywords": [
    "class",
    "extend",
    "parent",
    "inheritance",
    "inherit"
  ],
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "http://DUzun.Me/"
  },
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/classifyed.js/raw/master/LICENSE"
    }
  ],
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/duzun/classifyed.js/issues"
  },
  "homepage": "https://github.com/duzun/classifyed.js",
  "scripts": {
    "verup": "verup",
    "build": "gccs classifyed.js"
  },
  "verup": {
    "files": [
      "classifyed.js",
      "bower.json",
      "README.md"
    ]
  },
  "devDependencies": {
    "gccs": "^1.2",
    "verup": "^1.6"
  },
  "jshintConfig": {
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false
  }
}
